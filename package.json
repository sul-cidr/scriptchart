{
  "name": "DASH",
  "version": "0.0.1",
  "description": "Repository for the DASH project on Syriac manuscripts",
  "homepage": "http://dash.stanford.edu/",
  "main": "index.js",
  "scripts": {
    "build": "bundle exec jekyll clean && JEKYLL_ENV=prod bundle exec jekyll build && NODE_ENV=production node_modules/.bin/webpack -p",
    "dev": "yarn concurrently \"bundle exec jekyll serve --baseurl /scriptchart -H 0.0.0.0\" \"node_modules/.bin/webpack --watch\"",
    "precommit": "lint-staged",
    "test": "node_modules/.bin/jest",
    "test:update": "node_modules/.bin/jest -u",
    "coverage": "node_modules/.bin/jest --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": ["prettier --write", "git add"]
  },
  "keywords": [],
  "author": "Scott Bailey, Peter Broadwell, Javier de la Rosa",
  "license": "MIT",
  "jest": {
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "setupFiles": ["./js/config/setupTests.js"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/js/components/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/js/components/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.19",
    "bulma": "^0.7.5",
    "mirador": "2.6.0",
    "react": "^16.14.0",
    "react-accessible-accordion": "^2.4.5",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^7.7.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^2.1.8",
    "react-sidebar": "3.0.2",
    "react-tabs": "^3.2.3",
    "react-transition-group": "^1.2.1",
    "reactabular-dnd": "8.16.0",
    "reactabular-table": "8.14.0",
    "reactjs-popup": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.3.0",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-prettier": "^3.4.1",
    "file-loader": "^3.0.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.9.0",
    "jest-serializer-enzyme": "^1.0.0",
    "lint-staged": "^8.2.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^7.0.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.3.11",
    "react-visibility-toggles": "^1.2.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass-loader": "^7.3.1",
    "script-loader": "^0.7.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-fix-style-only-entries": "^0.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"]
}
